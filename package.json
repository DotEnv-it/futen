{
  "name": "futen",
  "version": "0.0.0-prerelease.13",
  "main": "dist/cjs/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/DotEnv-it/futen.git"
  },
  "license": "MIT",
  "devDependencies": {
    "@types/bun": "latest",
    "@typescript-eslint/eslint-plugin": "^7.1.0",
    "@typescript-eslint/parser": "^7.1.0",
    "eslint": "^8.57.0",
    "eslint-config-standard": "^17.1.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-n": "^15.7.0",
    "eslint-plugin-prettier": "latest",
    "eslint-plugin-promise": "^6.1.1",
    "husky": "^9.0.11",
    "prettier": "^3.2.5",
    "tsup": "^8.0.2"
  },
  "peerDependencies": {
    "typescript": "^5.0.0"
  },
  "description": "A (very) lightweight web framework, using decorators, simplifying syntax",
  "keywords": [
    "bun",
    "web",
    "http",
    "server",
    "socket",
    "backend",
    "framework"
  ],
  "peerDependenciesMeta": {
    "typescript": {
      "optional": true
    }
  },
  "scripts": {
    "bake": "bun bake.ts",
    "lint:prettier:fix": "bunx prettier --write ./src/**/*.ts ./tests/**/*.test.ts",
    "lint:eslint": "bun eslint ./src ./tests --ext .ts",
    "lint:eslint:fix": "bun eslint ./src ./tests --ext .ts --fix",
    "lint": "bun lint:prettier:fix && bun lint:eslint",
    "lint:fix": "bun lint:prettier:fix && bun lint:eslint:fix",
    "release:dry": "bun lint:fix && bun bake && bun test && npm publish --dry-run"
  }
}
